<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exam"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/ex"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ex"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Exam" >
        <activity android:name="com.example.exam.Star_activity" >
            android:exported="true"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.exam.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Exam.NoActionBar" >
        </activity>
    </application>

</manifest>